{
	"name": "vasek-purchart/rabbit-mq-database-transaction-producer-bundle",
	"description": "Publish messages to RabbitMQ producer when database transaction was committed",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"doctrine/doctrine-bundle": "~1.3",
		"php-amqplib/rabbitmq-bundle": "~1.9",
		"symfony/config": "~3.0",
		"symfony/dependency-injection": "~3.2",
		"symfony/http-kernel": "~3.0",
		"symfony/yaml": "~3.0"
	},
	"require-dev": {
		"consistence/coding-standard": "2.0",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"matthiasnoback/symfony-dependency-injection-test": "2.1",
		"phing/phing": "2.16",
		"phpunit/phpunit": "6.2.3"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\RabbitMqDatabaseTransactionProducerBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\RabbitMqDatabaseTransactionProducerBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
