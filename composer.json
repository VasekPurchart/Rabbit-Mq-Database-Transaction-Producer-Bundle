{
	"name": "vasek-purchart/rabbit-mq-database-transaction-producer-bundle",
	"description": "Publish messages to RabbitMQ producer when database transaction was committed",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "^7.2",
		"doctrine/dbal": "^2.11|^3.0",
		"doctrine/doctrine-bundle": "^1.6.4",
		"php-amqplib/rabbitmq-bundle": "^1.9",
		"psr/log": "^1.0",
		"symfony/config": "^3.0|^4.0",
		"symfony/dependency-injection": "^3.2|^4.0",
		"symfony/http-kernel": "^3.0|^4.0",
		"symfony/yaml": "^3.0|^4.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.10.1",
		"matthiasnoback/symfony-dependency-injection-test": "4.3",
		"php-parallel-lint/php-console-highlighter": "1.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phing/phing": "2.17.2",
		"phpunit/phpunit": "8.5.25",
		"shipmonk/composer-dependency-analyser": "1.8.1"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\RabbitMqDatabaseTransactionProducerBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\RabbitMqDatabaseTransactionProducerBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin",
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": false
		}
	}
}
